<Panel ux:Class="NavBar" Height="40" Color="Color.Text">
	<WhileCanGoBack>
		<BackButton Alignment="CenterLeft">
			<Clicked>
				<GoBack/>
			</Clicked>
			<AddingAnimation>
				<Move X="-1" RelativeTo="Size" Duration="0.8" Easing="CubicIn"/>
			</AddingAnimation>
			<RemovingAnimation>
				<Move X="-1" RelativeTo="Size" Duration="0.8" Easing="CubicOut"/>
			</RemovingAnimation>
		</BackButton>
	</WhileCanGoBack>
</Panel>
